<h1>New Portfolio form</h1>


<%= form_for(@portfolio) do |f| %>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :subtitle %>
    <%= f.text_field :subtitle %>
  </div>
  
  <div class="field">
    <%= f.label :body %>
    <%= f.text_area :body %>
  </div>
  
  <ul>
    <%= f.fields_for :technologies
    <li></li>
  </ul>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
